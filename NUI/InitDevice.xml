<OpenNI>
	<Licenses>
		<License vendor="PrimeSense" key="insert key here"/>
	</Licenses>
	<Log writeToConsole="true" writeToFile="false">
		<!-- 0 - Verbose, 1 - Info, 2 - Warning, 3 - Error (default) -->
		<LogLevel value="3"/>
		<Masks>
			<Mask name="ALL" on="true"/>
		</Masks>
		<Dumps>
		</Dumps>
	</Log>
	<ProductionNodes>
		<Node type="Depth" name="Depth1" stopOnError="false">
			<Configuration>
				<MapOutputMode xRes="320" yRes="240" FPS="30"/>
				<Mirror on="true"/>
			</Configuration>
		</Node>
		<Node type="User" />
		<Node type="Image" name="Image1" stopOnError="false">
			<Configuration>
			    <MapOutputMode xRes="640" yRes="480" FPS="30"/>
				<Mirror on="true"/>
			</Configuration>
		</Node>
	</ProductionNodes>
</OpenNI>
